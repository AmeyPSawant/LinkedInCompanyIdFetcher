{
    "manifest_version": 3,
    "name": "LinkedIn Company Fetcher",
    "version": "1.0",
    "description": "Fetches the first 'fsd_company:' value from a LinkedIn page",
    "permissions": ["activeTab", "identity"],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/company/*"],
        "js": ["content.js"]
      }
    ],
    "oauth2": {
      "client_id": "698204232750-cadujlp5em5nbuo3ftn893s9lmg0318e.apps.googleusercontent.com",
      "scopes": ["https://www.googleapis.com/auth/spreadsheets"]
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  